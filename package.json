{
   "name": "graphql-typescript-server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.1",
      "@types/ioredis": "^3.2.13",
      "@types/jest": "^23.3.0",
      "@types/node": "^10.3.5",
      "@types/uuid": "^3.4.3",
      "@types/yup": "^0.24.7",
      "gql2ts": "^1.8.1",
      "jest": "^23.4.1",
      "ts-jest": "^23.0.1",
      "ts-node": "7.0.0",
      "tslint": "^5.11.0",
      "tslint-config-prettier": "^1.14.0",
      "typescript": "2.9.2"
   },
   "dependencies": {
      "@gql2ts/from-query": "^1.8.1",
      "@gql2ts/from-schema": "^1.8.1",
      "bcrypt": "^3.0.0",
      "bcryptjs": "^2.4.3",
      "graphql": "^0.13.2",
      "graphql-import": "^0.6.0",
      "graphql-request": "^1.6.0",
      "graphql-tools": "^3.0.5",
      "graphql-yoga": "^1.14.12",
      "ioredis": "^3.2.2",
      "nodemon": "^1.18.3",
      "pg": "^7.4.3",
      "reflect-metadata": "^0.1.12",
      "typeorm": "0.2.7",
      "uuid": "^3.3.2",
      "yup": "^0.26.0"
   },
   "scripts": {
      "start": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
      "test": "NODE_ENV=test jest --forceExit",
      "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
   },
   "jest": {
      "globalSetup" : "./src/test-setup/callSetup.js",
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
